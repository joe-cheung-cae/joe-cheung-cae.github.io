{
  "extends": "astro/tsconfigs/strict",
  "compilerOptions": {
    "baseUrl": ".",
    "paths": {
      "@/*": ["src/*"],
      "@/components/*": ["src/components/*"],
      "@/layouts/*": ["src/layouts/*"],
      "@/utils/*": ["src/utils/*"],
      "@/content/*": ["src/content/*"],
      "@/styles/*": ["src/styles/*"]
    },
    "jsx": "react-jsx",
    "jsxImportSource": "preact"
  }
}
